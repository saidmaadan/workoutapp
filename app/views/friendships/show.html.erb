<div class="container">
  <div class="row">
    <div class="col-md-8">
      <p id="notice"><%= notice %></p>
      <h1><%= @friend.full_name %>'s Exercises</h1>
      <%= render "shared/user-exercise" %>
      <br>
      <%= content_tag :div, "", id: "chart", data: {workouts: @exercises} %>
